{"ast":null,"code":"import { subWeeks, setHours, setMinutes } from \"date-fns\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      eventName: \"\",\n      startDate: new Date()\n    };\n  },\n  methods: {\n    generate() {\n      // Generate the start and end dates dynamically using this.startDate\n      const startDate = setMinutes(setHours(this.startDate, 8), 0);\n      const endDate = setMinutes(setHours(this.startDate, 9), 0);\n      this.$ics.addEvent(\"en-us\", \"James\", \"\", \"\", startDate, endDate, \"\", {\n        name: \"Marilyn Stewart\",\n        email: \"\"\n      });\n\n      // Generate the second event's dates dynamically based on this.startDate\n      const startDate2 = subWeeks(startDate, 2);\n      const endDate2 = subWeeks(endDate, 2);\n      this.$ics.addEvent(\"en-us\", \"James 2\", \"\", \"\", startDate2, endDate2, \"\", {\n        name: \"Marilyn Stewart\",\n        email: \"\"\n      });\n      this.$ics.download(\"James\");\n      // console.log(this.$ics.calendar());\n    }\n  }\n};","map":{"version":3,"names":["subWeeks","setHours","setMinutes","name","data","eventName","startDate","Date","methods","generate","endDate","$ics","addEvent","email","startDate2","endDate2","download"],"sources":["src/App.vue"],"sourcesContent":["<template>\n    <div id=\"app\">\n        <div class=\"wrap\">\n            <!-- text input -->\n            <label>\n                Event name\n                <input\n                    v-model=\"eventName\"\n                    type=\"text\"\n                    id=\"start\"\n                    name=\"start\"\n                />\n            </label>\n\n            <label>\n                Date\n                <input\n                    v-model=\"startDate\"\n                    type=\"date\"\n                    id=\"start\"\n                    name=\"start\"\n                    style=\"margin-bottom: 15px\"\n                />\n            </label>\n\n            <a href=\"#\" @click=\"generate\">Generate</a>\n            <!-- <a href=\"#\" onclick=\"javascript:cal.download('Back date')\">Generate</a> -->\n        </div>\n    </div>\n</template>\n\n<script>\nimport { subWeeks, setHours, setMinutes } from \"date-fns\";\n\nexport default {\n    name: \"App\",\n    data() {\n        return {\n            eventName: \"\",\n            startDate: new Date(),\n        };\n    },\n    methods: {\n        generate() {\n            // Generate the start and end dates dynamically using this.startDate\n            const startDate = setMinutes(setHours(this.startDate, 8), 0);\n            const endDate = setMinutes(setHours(this.startDate, 9), 0);\n\n            this.$ics.addEvent(\n                \"en-us\",\n                \"James\",\n                \"\",\n                \"\",\n                startDate,\n                endDate,\n                \"\",\n                {\n                    name: \"Marilyn Stewart\",\n                    email: \"\",\n                }\n            );\n\n            // Generate the second event's dates dynamically based on this.startDate\n            const startDate2 = subWeeks(startDate, 2);\n            const endDate2 = subWeeks(endDate, 2);\n\n            this.$ics.addEvent(\n                \"en-us\",\n                \"James 2\",\n                \"\",\n                \"\",\n                startDate2,\n                endDate2,\n                \"\",\n                {\n                    name: \"Marilyn Stewart\",\n                    email: \"\",\n                }\n            );\n\n            this.$ics.download(\"James\");\n            // console.log(this.$ics.calendar());\n        },\n    },\n};\n</script>\n\n<style>\n#app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: center;\n    color: #2c3e50;\n    margin-top: 60px;\n}\n\n.wrap {\n    margin: auto;\n    position: absolute;\n    top: 0;\n    left: 0;\n    bottom: 0;\n    right: 0;\n    height: 50px;\n    width: 100px;\n}\n</style>\n"],"mappings":"AAgCA,SAAAA,QAAA,EAAAC,QAAA,EAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,SAAA,MAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,SAAA;MACA;MACA,MAAAH,SAAA,GAAAJ,UAAA,CAAAD,QAAA,MAAAK,SAAA;MACA,MAAAI,OAAA,GAAAR,UAAA,CAAAD,QAAA,MAAAK,SAAA;MAEA,KAAAK,IAAA,CAAAC,QAAA,CACA,SACA,SACA,IACA,IACAN,SAAA,EACAI,OAAA,EACA,IACA;QACAP,IAAA;QACAU,KAAA;MACA,CACA;;MAEA;MACA,MAAAC,UAAA,GAAAd,QAAA,CAAAM,SAAA;MACA,MAAAS,QAAA,GAAAf,QAAA,CAAAU,OAAA;MAEA,KAAAC,IAAA,CAAAC,QAAA,CACA,SACA,WACA,IACA,IACAE,UAAA,EACAC,QAAA,EACA,IACA;QACAZ,IAAA;QACAU,KAAA;MACA,CACA;MAEA,KAAAF,IAAA,CAAAK,QAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}