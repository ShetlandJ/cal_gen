</div>\n    </div>\n</template>\n\n<script>\nimport { sub } from \"date-fns\";\n\nexport default {\n    name: \"App\",\n    data() {\n        return {\n            eventName: \"\",\n            startDate: new Date(),\n        };\n    },\n    methods: {\n        generate() {\n            // this.$ics.addEvent(language, subject, description, location, begin, stop, url, organizer, rrule)\n\n            this.$ics.addEvent(\n                \"en-us\",\n                this.eventName,\n                \"\",\n                \"\",\n                this.startDate,\n                this.startDate,\n                '',\n                {\n                    name: \"Marilyn Stewart\",\n                    email: \"\",\n                }\n            );\n            this.$ics.addEvent(\n                \"en-us\",\n                this.eventName,\n                \"\",\n                \"\",\n                sub(this.startDate, { weeks: 2 }),\n                sub(this.startDate, { weeks: 2 }),\n                '',\n                {\n                    name: \"Marilyn Stewart\",\n                    email: \"\",\n                },\n            );\n\n            this.$ics.download(this.eventName);\n        },\n    },\n};\n</script>\n\n<style>\n#app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: center;\n    color: #2c3e50;\n    margin-top: 60px;\n}\n\n.wrap {\n    margin: auto;\n    position: absolute;\n    top: 0;\n    left: 0;\n    bottom: 0;\n    right: 0;\n    height: 50px;\n    width: 100px;\n}\n</style>\n"],"mappings":"AAmCA,SAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,SAAA,MAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,SAAA;MACA;;MAEA,KAAAC,IAAA,CAAAC,QAAA,CACA,SACA,KAAAN,SAAA,EACA,IACA,IACA,KAAAC,SAAA,EACA,KAAAA,SAAA,EACA,IACA;QACAH,IAAA;QACAS,KAAA;MACA,CACA;MACA,KAAAF,IAAA,CAAAC,QAAA,CACA,SACA,KAAAN,SAAA,EACA,IACA,IACAH,GAAA,MAAAI,SAAA;QAAAO,KAAA;MAAA,I